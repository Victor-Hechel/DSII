<h1 id='a'>Comunicadores</h1>
<div class="row">
	<div class="col-sm-offset-3 col-sm-6" id="fundo">
		<% @comunicadores.each do |comunicador| %>
			<div class="row">
				<div class="col-sm-12">
					<img class="col-sm-5" src="uploads/comunicadores/<%=comunicador.formaImagem()%>">
					<h3 class="col-sm-7"><%=comunicador.nome %></h3>
				</div>
			</div>
			
		<% end %>
	</div>
</div>